---
import { TabsList as TabsListReact } from '@/components/ui/tabs';

interface Props {
  class?: string;
  className?: string;
}

const { class: classAstro, className, ...props } = Astro.props;
---

<TabsListReact 
  client:load 
  class={classAstro} 
  className={className}
  {...props}
>
  <slot />
</TabsListReact> 