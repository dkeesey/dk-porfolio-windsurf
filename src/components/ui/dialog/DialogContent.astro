---
import { DialogContent as DialogContentReact } from '@/components/ui/dialog';

interface Props {
  class?: string;
  className?: string;
}

const { class: classAstro, className, ...props } = Astro.props;
---

<DialogContentReact client:load class={classAstro} className={className} {...props}>
  <slot />
</DialogContentReact> 