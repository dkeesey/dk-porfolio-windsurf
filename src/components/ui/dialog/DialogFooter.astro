---
import { DialogFooter as DialogFooterReact } from '@/components/ui/dialog';

interface Props {
  class?: string;
  className?: string;
}

const { class: classAstro, className, ...props } = Astro.props;
---

<DialogFooterReact client:load class={classAstro} className={className} {...props}>
  <slot />
</DialogFooterReact> 