---
import { DialogDescription as DialogDescriptionReact } from '@/components/ui/dialog';

interface Props {
  class?: string;
  className?: string;
}

const { class: classAstro, className, ...props } = Astro.props;
---

<DialogDescriptionReact client:load class={classAstro} className={className} {...props}>
  <slot />
</DialogDescriptionReact> 