---
import { Container as ContainerReact } from './container';

interface Props {
  as?: 'div' | 'section' | 'main';
  class?: string;
  className?: string;
}

const { as, class: classAstro, className, ...props } = Astro.props;
---


<ContainerReact client:load as={as} class={classAstro} className={className} {...props}>
  <slot />
</ContainerReact> 