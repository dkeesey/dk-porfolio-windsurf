---
import { Layout as LayoutReact } from './Layout';

interface Props {
  seo?: {
    title?: string;
    description?: string;
    image?: string;
    type?: 'website' | 'article';
    publishDate?: Date;
    author?: string;
  };
}

const { seo } = Astro.props;
--- 

<LayoutReact client:load seo={seo}>
  <slot />
</LayoutReact> 